<!DOCTYPE html>
<html lang="fr" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Profile</title>
  </head>
  <body>
    <!-- Navbar content -->
    <a href="/">Home</a>
    <a href="/admin"><%= admin.prenom %></a>
    <a href="/deconnect">Déconnection</a>
    <!-- Navbar ends -->

    <h1>Add Contact</h1>
    <form action="/add_contact" method="post">
      <input type="text" name="nom" placeholder="nom" required="true"><br>
      <input type="text" name="prenom" placeholder="prenom" required="true" ><br>
      <input type="text" name="fonction" placeholder="fonction" required="true" ><br>
      <input type="text" name="rattach" placeholder="rattachement" required="true" ><br>
      <input type="text" name="mail" placeholder="mail" required="true" ><br>
      <input type="text" name="tel" placeholder="téléphone" required="true" ><br>
      <input type="submit" name="submit" value="Ajouter">
    </form>
    <% if (ps != "undefined") { %>
      <%= ps %>
      <% } %>
  </body>
</html>
